{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wfrm-resources": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "wfrm",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/wfrm-resources",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/dev-config",
              "src/manifest.json",
              "src/refresh-token.html"
            ],
            "styles": [
              "src/styles/main.scss",
              "node_modules/@busacca/ng-pick-datetime/assets/style/picker.scss",
              "node_modules/multiple-select/dist/multiple-select.css"
            ],
            "scripts": [
              "node_modules/gsap/src/uncompressed/TweenMax.js",
              "node_modules/jquery/dist/jquery.slim.js",
              "node_modules/multiple-select/dist/multiple-select.js",
              "src/assets/js/visualize.js/visualize.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/dev-config",
                "src/manifest.json",
                "src/refresh-token.html"
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wfrm-resources:build",
            "port": 11200
          },
          "configurations": {
            "production": {
              "browserTarget": "wfrm-resources:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wfrm-resources:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverageExclude": [
              "src/app/test/**",
              "src/app/store/**",
              "src/app/directives/**",
              "src/app/interceptors/**",
              "src/app/utils/**",
              "src/app/containers/application-root/**",
              "src/app/services/**",
              "src/app/containers/**",
              "src/app/components/unauthorized-page/*.ts",
              "src/app/components/sign-out-page/*.ts",
              "src/app/components/group-detail/sections/group-detail-rosters/*.ts",
              "src/app/components/personnel-detail/sections/personnel-detail-rosters/*.ts",
              "src/app/components/resources/**"
            ],
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles/main.scss",
              "node_modules/@busacca/ng-pick-datetime/assets/style/picker.scss",
              "node_modules/multiple-select/dist/multiple-select.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.slim.js",
              "node_modules/multiple-select/dist/multiple-select.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.json",
              "src/refresh-token.html"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "wfrm-resources-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "wfrm-resources:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wfrm-resources:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "wfrm-resources"
}
