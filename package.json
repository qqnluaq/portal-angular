{
  "name": "wfone-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ng serve",
    "start.prod": "ng serve --prod --sourceMap=true",
    "build": "ng build",
    "build.prod": "ng build --prod --sourceMap=true --base-href /pub/wfrm-resources/ && replace-in-file /ngsw:/g ngsw:wfrme: dist/wfrm-resources/ngsw-worker.js --verbose --isRegex",
    "test": "ng test",
    "test-and-coverage": "ng test --browsers ChromeHeadlessCustom --watch=false --code-coverage",
    "test-dev": "ng test --reporters=mocha",
    "test-dev-chrome": "ng test --watch=true --browsers=Chrome",
    "test-dev-safari": "ng test --watch=true --browsers=Safari",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cr": "npm run lint && npm start.prod",
    "mavenbuild": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --prod --output-path ../../../target/angular --base-href /pub/wfrm-resources/ && replace-in-file /ngsw:/g ngsw:wfrme: ../../../target/angular/ngsw-worker.js --verbose --isRegex",
    "bundle-report": "webpack-bundle-analyzer dist/wfrm-resources/stats.json",
    "compodoc": "npx compodoc -p src/tsconfig.app.json",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/pwa": "^0.10.4",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@busacca/ng-pick-datetime": "^9.0.2",
    "@ngrx/effects": "^9.1.2",
    "@ngrx/entity": "^9.1.2",
    "@ngrx/router-store": "^9.1.2",
    "@ngrx/store": "^9.1.2",
    "@ngrx/store-devtools": "^9.1.2",
    "@wf1/core-ui": "^2.1.19",
    "@wf1/orgunit-rest-api": "0.0.4",
    "angular-oauth2-oidc": "^5.0.2",
    "angular-sanitize": "^1.7.7",
    "angular2-uuid": "^1.1.1",
    "axe-core": "^3.5.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "karma-viewport": "^1.0.4",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "ng-connection-service": "^1.0.4",
    "ngx-mask": "^9.1.2",
    "ngx-moment": "^3.5.0",
    "ngx-pagination": "^5.0.0",
    "ngx-trim": "^1.0.1",
    "puppeteer": "^1.14.0",
    "rxjs": "^6.3.3",
    "sass": "^1.26.5",
    "scroll-into-view-if-needed": "^2.2.25",
    "vanilla-text-mask": "^5.1.1",
    "wicg-inert": "^3.0.3",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular-devkit/build-optimizer": "^0.10.2",
    "@angular/cli": "^9.1.7",
    "@angular/compiler": "^9.1.9",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^7.0.0",
    "@types/gsap": "^1.20.2",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/jquery": "^3.5.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.32",
    "protractor": "^7.0.0",
    "replace-in-file": "^3.4.2",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "typescript": "3.8.3"
  }
}
